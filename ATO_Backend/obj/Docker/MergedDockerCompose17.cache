name: dockercompose14811200894164250809
services:
  ato_api:
    build:
      context: D:\Job\Tháº¯ng\clonebe\ATO_BackEnd\ATO_Backend
      dockerfile: ATO_API/Dockerfile
    container_name: atoapi
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5000"
      ASPNETCORE_HTTPS_PORTS: "5001"
    image: atoapi
    networks:
      default: null
    platform: windows/amd64
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\-ASUS-\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\-ASUS-\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  database:
    container_name: learning_docker_database
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_DB: learning_docker_database
      MSSQL_PID: Express
      SA_PASSWORD: "123456"
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    restart: always
networks:
  app-network:
    name: dockercompose14811200894164250809_app-network
    driver: bridge
  default:
    name: dockercompose14811200894164250809_default